#content ul {
    margin: 4px 0px 4px 0px;
}

#content ul > li {
    margin: 4px 0px;
}

/* Links */
a {
    text-decoration: none;
}

a:hover{
    text-decoration: underline;
}

/* Navigation */
#nav {
    position: absolute;
    background-image: url( /images/nav.png );
    background-repeat: no-repeat;
    height: 350px;
    width: 145px;
    top: 162px;
    left: 20px;
    padding: 20px 0px 0px 20px;
    font-size: 11px;
    line-height: 17px;
}

