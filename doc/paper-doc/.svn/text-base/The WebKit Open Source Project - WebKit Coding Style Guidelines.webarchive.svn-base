    margin: 0px 0px 0px -5px;
    padding: 0px;
    list-style-type: none;
}

#navlist a {
    color: #333;
}

#navlist .subtitle {
    font-weight: bold;
    margin-top: 10px;
}

#navlist li a:hover, #navlist li a:active {
    text-decoration: underline;
}

#notice, #terms {
    min-width: 0px;
    padding: 0px;
    margin: 30px 10px 10px -5px;
    font-size: 9px;
    line-height: 11px;
    color: #aaa !important;
}

#notice a {
    color: inherit !important;
    text-decoration: none;
}

#notice a:hover {
    color: #555 !important;
    text-decoration: none;
}

dt { 
    font-weight: bold;
    margin-top: 1em;
}

.code, pre, tt, code {
    font-family: Courier, Fixed;
    font-size: 110%;
}

.code {
    margin-left: 2em;
    font-size: 90%;
    line-height: normal;
}

.mac-instructions, .windows-instructions {
    border: 1px solid black;
    background-color: white;
    -webkit-border-radius: 5px;
    padding: 0 1em;
    margin: 1em 0;
